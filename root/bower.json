{
    "name"      : "fcoo-maps-{%= name %}",
    "version"   : "0.0.0",
    "homepage"  : "https://github.com/FCOO/fcoo-maps-{%= name %}",
    "authors": [
        "{%= author_name %}"
    ],
    "description": "{%= description %}",
    "main": [
        "dist/fcoo-maps-{%= name %}.js",
        "dist/fcoo-maps-{%= name %}.css"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/FCOO/fcoo-maps-{%= name %}.git"
    },

    "dependencies": {
        "fcoo-maps": "fcoo/fcoo-maps#latest"
    },
    "OR_dependencies": {
        "fcoo-maps-time": "fcoo/fcoo-maps-time#latest"
    },

    "devDependencies": {
        "bourbon": "latest",
        "modernizr-mixin": "latest",
        "mathsass": "latest"
  },
    
    "license": "MIT",

    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "moduleType": ["es6"]
}
